{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": "off",
    "implicit-arrow-linebreak": "off",
    "arrow-body-style": "off",
    "no-param-reassign": "off",
    "comma-dangle": "off",
    "import/extensions": "always"
  }
}
